value = raw_input("Please enter a number between 1 and 100: ")
value = int(value)

try:
    for number in range(1, value+1):
        if number % 3 == 0 and number % 5 == 0:
         print "fizzbuzz"
        elif number % 3 == 0:
            print "fizz"
        elif number % 5 == 0:
            print "buzz"
        else:
            print number
except Exception as e:
    print "Please enter a number!"